{
  "name": "shapes-area-api",
  "version": "1.0.0",
  "description": "An API to calulate areas of shapes",
  "main": "index.js",
  "scripts": {
    "dev": "pm2 start ./node_modules/sucrase/bin/sucrase-node --name shapes-area-api-dev -- ./src/server.ts --watch --color=true && pm2 log",
    "prebuild:dev": "rm -rf build",
    "build:dev": "tsc",
    "postbuild:dev": "pm2 start ecosystem.config.json && pm2 log",
    "prebuild": "rm -rf build",
    "build": "tsc",
    "start": "npm run build",
    "postbuild": "ls && npm run migrate && npm run seed && pm2-runtime start ecosystem.config.json",
    "stop": "pm2 delete all",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Omobolanle-Arogundade/asb-coding-assignment.git"
  },
  "author": "Omobolanle Aro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Omobolanle-Arogundade/asb-coding-assignment/issues"
  },
  "homepage": "https://github.com/Omobolanle-Arogundade/asb-coding-assignment#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcryptjs": "^2.4.3",
    "cli-table": "^0.3.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^5.0.0",
    "find-config": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "pluralize": "^8.0.0",
    "pm2": "^4.5.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.26",
    "@types/cors": "^2.8.4",
    "@types/crypto-js": "^3.1.43",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.10.43",
    "@types/pluralize": "0.0.29",
    "@types/validator": "^10.11.0",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "husky": "^4.3.4",
    "lint-staged": "^10.5.1",
    "prettier": "^1.16.4",
    "sucrase": "^3.10.0",
    "typescript": "^3.3.3333"
  }
}
